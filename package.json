{
  "name": "brunch-hugo-coffee-bootstrap-theme",
  "description": "Brunch - A Hugo theme with Brunch, CoffeeScript, Bootstrap and many more batteries included",
  "author": "Jörg Kütemeier",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkuetemeier/brunch-hugo-coffee-bootstrap-theme"
  },
  "scripts": {
    "clean": "rm -rf ./static/*",
    "server": "brunch watch --server",
    "start": "brunch watch",
    "build": "brunch build --production",
    "debug": "brunch build --debug",
    "prepare:bootstrap": "cp -r node_modules/bootstrap",
    "test-postinstall": "cp -r node_modules/font-awesome/fonts app/assets/fonts && brunch b"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.1",
    "font-awesome": "^4.5.0",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.0.0",
    "babel-brunch": "^6.0.2",
    "brunch": "^2.4.2",
    "clean-css-brunch": "^2.0.0",
    "coffee-script": "^1.10.0",
    "coffee-script-brunch": "^2.0.0",
    "copycat-brunch": "^1.1.0",
    "css-brunch": "^2.0.0",
    "javascript-brunch": "^2.0.0",
    "less-brunch": "^2.0.0",
    "uglify-js-brunch": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
